<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.innovative.dao.PoiDao">






	<!--添加专家信息-->
	<insert id="addPoiExpert" parameterType="Expert">
		INSERT INTO "Experts"
		("Id", "Avatar", "Contact", "CooperationStatus", "CreatedAt", "CreatedBy", "Deleted", "DeletedAt", "DeletedBy", "Education", "HFactor", "IsActive", "JobTitle", "Name", "Rank", "ResearchAchievement", "ResearchDirection", "Resume", "RowVersion", "Sectors", "Unit", "UpdatedAt", "UpdatedBy", "File")
	VALUES (#{id}, #{avatar}, #{contact}, #{cooperationStatus}, #{createdAt}, '', #{deleted}, #{createdAt}, #{deletedBy}, #{education}, #{hFactor}, #{isActive}, #{jobTitle}, #{name}, #{rank}, #{researchAchievement}, #{researchDirection}, #{resume}, #{rowVersion}, '${sectors}', #{unit}, #{createdAt}, #{updatedBy}, '${file}')
	</insert>








	

</mapper>